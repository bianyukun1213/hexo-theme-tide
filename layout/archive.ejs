<% const tideCtx=get_ctx(site,config,theme,page) %>
<div id="tide-archive-posts">
  <% let dateBegin,dateEnd %>
  <% if(is_archive()){ %>
  <% if(is_month()){ %>
  <% dateBegin=moment().year(tideCtx.archive_year).month(parseInt(tideCtx.archive_month)-1).date(1) %>
  <% dateEnd=moment(dateBegin).add(1,'month').add(-1,'days') %>
  <h1 class="un-m-0 un-mb-4 un-text-4xl un-font-bold"><%= __('page_title.archive_month',date(dateBegin),date(dateEnd)) %></h1>
  <% }else if(is_year()){ %>
  <% dateBegin=moment().year(tideCtx.archive_year).month(0).date(1) %>
  <% dateEnd=moment(dateBegin).add(12,'month').add(-1,'days') %>
  <h1 class="un-m-0 un-mb-4 un-text-4xl un-font-bold"><%= __('page_title.archive_year',date(dateBegin),date(dateEnd)) %></h1>
  <% }else{ %>
  <h1 class="un-m-0 un-mb-4 un-text-4xl un-font-bold"><%= __('page_title.archive') %></h1>
  <% } %>
  <% } %>
  <% if(is_archive()){ %>
  <% if(dateBegin&&dateEnd){ %>
  <%- partial('./partials/post-ul.ejs',{tideCtx,postSrc:site,filter:p=>p.date.isSameOrAfter(dateBegin)&&p.date.isSameOrBefore(dateEnd)}) %>
  <% }else{ %>
  <%- partial('./partials/post-ul.ejs',{tideCtx,postSrc:site}) %>
  <% } %>
  <% }else{ %>
  <%- partial('./partials/post-ul.ejs',{tideCtx,postSrc:site}) %>
  <% } %>
</div>
<div id="tide-archive-paginator">
  <%- partial('./partials/paginator.ejs') %>
</div>